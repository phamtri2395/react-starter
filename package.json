{
  "name": "redux-starter",
  "version": "0.3.0",
  "description": "Redux Starter Kit",
  "main": "gulp.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "// 1": [
      "Start server"
    ],
    "start": "gulp --gulpfile tools/gulp.js run",
    "server": "npm run build:prod && npm run build:server && cross-env mode=server node ./dist/server.js",
    "// 2": [
      "Linter"
    ],
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint -c ./tools/config/.eslintrc --color ./src/**",
    "eslint:fix": "eslint -c ./tools/config/.eslintrc --color ./src/**/*.js* --ignore-pattern=*.json --fix",
    "stylelint": "stylelint --config ./tools/config/.stylelintrc \"src/**/*.css\"",
    "// 3": [
      "Build"
    ],
    "build": "gulp --gulpfile tools/gulp.js build",
    "build:dev": "cross-env mode=dev npm run build",
    "build:prod": "cross-env mode=prod npm run build",
    "build:server": "cross-env mode=server webpack --config tools/config/webpack.config.server.js",
    "// 4": [
      "Document"
    ],
    "doc:components": "gulp --gulpfile tools/gulp.js doc-react-component",
    "story": "styleguidist server --config ./tools/config/styleguide.config.js",
    "story:build": "styleguidist build --config ./tools/config/styleguide.config.js",
    "// 5": [
      "Version control"
    ],
    "version:patch:tag": "git add . && npm version patch -m \"Patch version & tag %s\"",
    "version:minor:tag": "git add . && npm version minor -m \"Update version & tag %s\"",
    "version:major:tag": "git add . && npm version major -m \"Upgrade version & tag %s\"",
    "version:patch": "git add . && npm --no-git-tag-version version patch && git add . && git commit -m \"Patch version & commit\"",
    "version:minor": "git add . && npm --no-git-tag-version version minor && git add . && git commit -m \"Update version & commit\"",
    "version:major": "git add . && npm --no-git-tag-version version major && git add . && git commit -m \"Upgrade version & commit\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phamtri2395/redux-starter.git"
  },
  "author": "phamtri2395@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phamtri2395/redux-starter/issues"
  },
  "homepage": "https://github.com/phamtri2395/redux-starter#readme",
  "dependencies": {
    "browser-sync": "^2.18.8",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
