    "prop-types": "^15.5.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "recompose": "^0.23.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "superagent": "^3.5.2",
    "webpack": "^2.3.2"
  },
